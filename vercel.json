{
  "buildCommand": "cd client/frontend && npm run build",
  "outputDirectory": "client/frontend/dist",
  "framework": null,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@18"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}